{
	"name": "godot-d",
	"description": "Godot game engine language bindings",
	"authors": [ "sheepandshepherd" ],
	"license": "MIT",
	
	"targetType": "library",
	"dflags-linux-dmd": ["-fPIC"],
	"dflags-linux-ldc2": ["-relocation-model=pic"],
	
	"importPaths": [ "src", "classes", "common" ],
	"sourcePaths": [ "src", "classes", "common" ],
	
	"subPackages":
	[
		{
			"name": "generator",
			"description": "A program that binds Godot's C++ classes and GDNative C functions",
			"dependencies":
			{
				"asdf": "~>0.1.8",
				"dxml": "~>0.2.3",
			},
			"sourcePaths": [ "generator", "common" ]
		},
		{
			"name": "miscclasses",
			"description": "Rarely useful classes separated from the main package for organization",
			"license": "MIT",
			"dependencies":
			{
				"godot-d": "*",
			},
			"targetType": "library",
			"dflags-linux-dmd": ["-fPIC"],
			"dflags-linux-ldc2": ["-relocation-model=pic"],
			"importPaths": [ "miscclasses" ],
			"sourcePaths": [ "miscclasses" ],
		},
		"./examples/test/",
		"./examples/asteroids/",
	],
}
